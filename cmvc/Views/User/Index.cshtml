@model List<cmvc.Models.TableViewModels.UserTableVM>
@{
    ViewBag.Title = "Index";
}

<h2>Usuarios</h2>

<div class="row">
    <div class="col-lg-12">
        <table class="table table-bordered">
            <tr>
                <th>#</th>
                <th>Email</th>
                <th>Edad</th>
            </tr>

            @foreach (var oElemento in Model )
            {
                <tr>
                    <td>@oElemento.Id</td>
                    <td>@oElemento.Email</td>
                    <td>@oElemento.Edad</td>
                </tr>
            }

        </table>
    </div>
</div>
